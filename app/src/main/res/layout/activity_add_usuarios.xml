<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#ffc53a"
    android:layout_height="match_parent"
    tools:context=".admin.usuarios.AddUsuarios">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/seguisb"
        android:text="Add Usuario"
        android:textColor="#161032"
        android:textSize="60px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        tools:ignore="HardcodedText,MissingConstraints,PxUsage,SpUsage" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/seguisb"
        android:text="Click to Add Incidente"
        android:textColor="#161032"
        android:textSize="30px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.018"
        tools:ignore="HardcodedText,MissingConstraints,PxUsage" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:layout_marginTop="300px"
        android:background="@drawable/card_background"
        android:elevation="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints,PxUsage,UnusedAttribute">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="39dp"
            android:adjustViewBounds="false"
            android:onClick="openAdminSpecialty"
            android:src="@drawable/ic_back_blue"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="ContentDescription,OnClick,RtlHardcoded,UsingOnClickInXml" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"

            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:layout_marginBottom="0dp"
            tools:ignore="ObsoleteLayoutParam,RtlHardcoded,ScrollViewSize">


            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/animationView"
                android:layout_width="180dp"
                android:layout_height="180dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.963"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"
                app:lottie_rawRes="@raw/alert"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/nombre_usuario"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Nombre del Usuario"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.097"
                tools:ignore="Autofill,DuplicateIds,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <EditText
                android:id="@+id/email_usuario"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Email"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.194"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <EditText
                android:id="@+id/password_usuario"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:background="@drawable/edit_text_background"
                android:clickable="true"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="ContraseÃ±a"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.292"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownId,UnknownIdInLayout" />

            <EditText
                android:id="@+id/image_usuario"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Agregar Imagen"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.483"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <Spinner
                android:id="@+id/id_tipo_usuarioss"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Tipo de Usuario"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.388"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />


            <Button
                android:id="@+id/btnAddUser"
                android:layout_width="179dp"
                android:layout_height="46dp"
                android:layout_below="@+id/listaConversion"
                android:background="@drawable/button"
                android:text="Save"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="40px"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.605"
                tools:ignore="HardcodedText,UnknownIdInLayout" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>